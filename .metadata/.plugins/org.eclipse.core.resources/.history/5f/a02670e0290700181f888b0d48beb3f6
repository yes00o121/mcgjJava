		Thread thread2 = new Thread(new Runnable() {
			public void run() {
				for(int i=0;i<300000;i++){
					FileInputStream f = null;
					try {
						f = new FileInputStream("E://test.txt");
					} catch (FileNotFoundException e) {
						// TODO Auto-generated catch block
